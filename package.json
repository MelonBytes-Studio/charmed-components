{
  "name": "@rbxts/charmed-components",
  "version": "1.2.1",
  "description": "Library that adds sweet-charm atoms power to flamework components",
  "main": "out/init.luau",
  "type": "module",
  "typings": "out/index.d.ts",
  "files": [
    "out",
    "README.md",
    "flamework.build",
    "!out/tests",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MelonBytes-Studio/charmed-components.git"
  },
  "homepage": "https://github.com/MelonBytes-Studio/charmed-components#readme",
  "scripts": {
    "build": "rbxtsc",
    "devBuild": "rbxtsc --type game --rojo dev.project.json",
    "devWatch": "rbxtsc --watch --type game --rojo dev.project.json",
    "serve": "rojo serve dev.project.json",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "charm",
    "sweet-charm",
    "flamework",
    "components"
  ],
  "author": "sumer_real",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rbxts/compiler-types": "^3.0.0-types.0",
    "@rbxts/sweet-charm": "^1.3.0",
    "@rbxts/testez": "^0.4.2-ts.0",
    "@rbxts/types": "^1.0.813",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "isentinel-eslint-plugin-roblox-ts": "0.0.37",
    "prettier": "^3.4.2",
    "rbxts-transformer-flamework": "^1.2.3",
    "roblox-ts": "^3.0.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.18.2",
	"@flamework/components": "^1.2.3",
    "@flamework/core": "^1.2.3"
  },
  "peerDependencies": {
    "@rbxts/sweet-charm": "^1.3.0",
	"@flamework/components": "^1.2.3",
    "@flamework/core": "^1.2.3"
  },
  "dependencies": {
    "@rbxts/janitor": "1.17.0-ts.1",
    "@rbxts/services": "^1.5.5"
  }
}
